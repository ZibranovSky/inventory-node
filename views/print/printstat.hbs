<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

 
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
               
                <!-- End of Topbar -->

  <!-- Begin Page Content -->
                <div class="container-fluid">

                    <!-- Page Heading -->
                    <div class="d-sm-flex align-items-center justify-content-between mb-4">
                   
                        <h1 class="h3 mb-0 text-gray-800"></h1>
                   
                        
                       
                    </div>

                    <!-- Content Row -->
                    <div class="row">

                        <!-- Earnings (Monthly) Card Example -->
                        <div class="col-xl-3 col-md-6 mb-4">
                            <div class="card border-left-primary shadow h-100 py-2">
                                <div class="card-body">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col mr-2">
                                            <div class="text-xs font-weight-bold text-primary text-uppercase mb-1">
                                                Jumlah Karyawan</div>
                                                {{#each jumlahKaryawan}}
                                            <div class="h5 mb-0 font-weight-bold text-gray-800">{{jkaryawan}}</div>
                                            {{/each}}
                                        </div>
                                        <div class="col-auto">
                                            <i class="fas fa-users fa-2x"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Earnings (Monthly) Card Example -->
                        <div class="col-xl-3 col-md-6 mb-4">
                            <div class="card border-left-success shadow h-100 py-2">
                                <div class="card-body">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col mr-2">
                                            <div class="text-xs font-weight-bold text-success text-uppercase mb-1">
                                                Jumlah Supplier</div>
                                                {{#each jumlahSupplier}}
                                            <div class="h5 mb-0 font-weight-bold text-gray-800">{{ jsup }}</div>
                                            {{/each}}
                                        </div>
                                        <div class="col-auto">
                                            
                                            <i class="fas fa-parachute-box fa-2x"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Earnings (Monthly) Card Example -->
                        <div class="col-xl-3 col-md-6 mb-4">
                            <div class="card border-left-info shadow h-100 py-2">
                                <div class="card-body">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col mr-2">
                                            <div class="text-xs font-weight-bold text-info text-uppercase mb-1">Jumlah Rak
                                            </div>
                                            <div class="row no-gutters align-items-center">
                                                <div class="col-auto">
                                                    {{#each jumlahRak}}
                                                    <div class="h5 mb-0 mr-3 font-weight-bold text-gray-800">{{ jrak }}</div>
                                                    {{/each}}
                                                </div>
                                                <div class="col">
                                                        
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-auto">
                                            <i class="fas fa-warehouse fa-2x"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Pending Requests Card Example -->
                        <div class="col-xl-3 col-md-6 mb-4">
                            <div class="card border-left-warning shadow h-100 py-2">
                                <div class="card-body">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col mr-2">
                                            <div class="text-xs font-weight-bold text-warning text-uppercase mb-1">
                                                Jumlah Barang</div>
                                                {{#each jumlahBarang}}
                                            <div class="h5 mb-0 font-weight-bold text-gray-800">{{jbrg}}</div>
                                            {{/each}}
                                        </div>
                                        <div class="col-auto">
                                            <i class="fas fa-boxes fa-2x"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                            <div class="col-xl-3 col-md-6 mb-4">
                            <div class="card border-left-dark shadow h-100 py-2">
                                <div class="card-body">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col mr-2">
                                            <div class="text-xs font-weight-bold text-dark text-uppercase mb-1">
                                                Ajuan Barang</div>
                                                {{#each ajuanBarang}}
                                            <div class="h5 mb-0 font-weight-bold text-gray-800">{{jajuan}}</div>
                                            {{/each}}
                                        </div>
                                        <div class="col-auto">

                                            <i class="fas fa-clipboard-list fa-2x"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-xl-3 col-md-6 mb-4">
                            <div class="card border-left-success shadow h-100 py-2">
                                <div class="card-body">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col mr-2">
                                            <div class="text-xs font-weight-bold text-success text-uppercase mb-1">
                                                Barang Keluar / Disetujui</div>
                                                {{#each barangKeluar}}
                                            <div class="h5 mb-0 font-weight-bold text-gray-800">{{jkeluar}}</div>
                                            {{/each}}
                                        </div>
                                        <div class="col-auto">

                                            <i class="fas fa-handshake fa-2x"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    </div>

